/* For Loop that calculates squares for numbers 1-10
Brian McMillan
COSC-1436
Program 5-9
*/

#include <iostream>
using namespace std;

int main ()
{
  const int MIN_NUMBER = 1, //starting value
            MAX_NUMBER = 10; //ending value
  
  int num;
  
  cout << "Number Number Squared\n";
  cout << "-----------------------\n";
  
  for (num = MIN_NUMBER; num <= MAX_NUMBER; num++) \\initilization expression, test exp, update exp
    cout << num << "\t\t" << (num * num) << endl;
    
  return 0;
  
}
